{
  "name": "redstone-oracle-node",
  "version": "0.4.38",
  "description": "A data provider module that fetches price information and serves it to end users.",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc --build",
    "start": "ts-node index.ts",
    "start:prod": "node dist/index.js",
    "start:dev": "yarn start > tmp.out",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:ci": "jest --runInBand --ci --reporters='default' --reporters='./tools/GithubActionsReporter'",
    "test:main-dry-run": "DRY_RUN_TEST_TYPE=main yarn test:dry-run",
    "test:stocks-dry-run": "DRY_RUN_TEST_TYPE=stocks yarn test:dry-run",
    "test:avalanche-dry-run": "DRY_RUN_TEST_TYPE=avalanche yarn test:dry-run",
    "test:rapid-dry-run": "DRY_RUN_TEST_TYPE=rapid yarn test:dry-run",
    "test:dry-run": "jest --runInBand --config=jest.config.dry-run.ts --forceExit test/dry-run/dry-run-test.spec.ts",
    "prepublishOnly": "yarn test && yarn build",
    "version": "git add -A src",
    "postversion": "git push && git push --tags",
    "docker:build": "docker build -f Dockerfile.public -t redstone-node .",
    "run-ts": "ts-node"
  },
  "license": "MIT",
  "author": "Redstone Team <dev@redstone.finance>",
  "contributors": [
    "Jakub Wojciechowski <jakub@redstone.finance>",
    "Alex Suvorov <alex@redstone.finance>",
    "Peter Pedziwiatr <peter@redstone.finance>"
  ],
  "dependencies": {
    "@balancer-labs/sdk": "^1.1.1",
    "@bundlr-network/client": "^0.7.4",
    "@metamask/eth-sig-util": "^4.0.1",
    "arweave": "^1.10.23",
    "axios": "^0.21.1",
    "ccxt": "^3.0.92",
    "consola": "^2.15.3",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^16.0.1",
    "ecb-euro-exchange-rates": "^2.0.0",
    "ethereum-multicall": "^2.16.1",
    "ethereumjs-util": "^7.0.10",
    "ethers": "^5.6.9",
    "express": "^4.18.2",
    "git-last-commit": "^1.0.1",
    "jsonpath": "^1.1.1",
    "level": "^8.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "mongoose": "^6.7.0",
    "node-schedule": "^2.1.1",
    "prompts": "^2.4.2",
    "redstone-api": "^0.4.11",
    "sort-deep-object-arrays": "^1.1.2",
    "uuid": "^9.0.0",
    "yahoo-finance": "^0.3.7",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/exchange-rates-api": "^1.0.0",
    "@types/express": "^4.17.14",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^29.5.1",
    "@types/jsonpath": "^0.2.0",
    "@types/lodash": "^4.14.168",
    "@types/luxon": "^3.3.0",
    "@types/node": "^14.14.31",
    "@types/prompts": "^2.0.14",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.0",
    "ar-gql": "^0.0.11",
    "copyfiles": "^2.4.1",
    "dateformat": "^4.5.1",
    "deep-sort-object": "^1.0.2",
    "download": "^8.0.0",
    "ethereum-waffle": "^4.0.10",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "msw": "^1.2.1",
    "open": "^8.0.4",
    "plotly": "^1.0.6",
    "prettier": "^2.6.2",
    "prompts": "^2.4.2",
    "replace-in-file": "^6.3.2",
    "supertest": "^6.2.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
