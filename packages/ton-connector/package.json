{
  "name": "ton-connector",
  "prettier": "redstone-prettier-config",
  "version": "0.0.1",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "run-script": "npx blueprint run --testnet --mnemonic",
    "build": "rm -rf dist && tsc -build",
    "compile-adapter": "npx func-js contracts/adapter.fc --boc contracts/adapter.cell",
    "compile-feed": "npx func-js contracts/feed.fc --boc contracts/feed.cell",
    "deploy-adapter": "yarn compile-adapter && yarn run-script  deployAdapter",
    "deploy-feed": "yarn compile-feed && yarn run-script deployFeed",
    "get-key": "yarn run-script runAdapterGetKey",
    "init-adapter": "yarn run-script runAdapterInit",
    "fetch-feed-value": "yarn run-script runFeedFetchFeedValue",
    "get-feed-value": "yarn run-script runFeedGetFeedValue",
    "verify": "yarn run-script runAdapterVerify"
  },
  "dependencies": {
    "@orbs-network/ton-access": "^2.3.3",
    "@ton-community/func-js": "0.6.3-tvmbeta.1",
    "@ton-community/func-js-bin": "0.4.5-tvmbeta.1",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "redstone-sdk": "^1.0.9",
    "ton": "^13.5.0",
    "ton-core": "^0.49.1",
    "ton-crypto": "^3.2.0"
  },
  "devDependencies": {
    "@ton-community/blueprint": "^0.12.0",
    "@ton-community/sandbox": "0.12.0-tvmbeta.1",
    "@ton-community/test-utils": "^0.2.0",
    "@types/jest": "^29.5.3",
    "@types/node": "^18.13.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "references": [
    {
      "path": "../protocol"
    },
    {
      "path": "../sdk"
    }
  ]
}
